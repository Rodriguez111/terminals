        <url-pattern>/terminalinputvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInputValidate</servlet-name>
        <url-pattern>/givevalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveValidate</servlet-name>
        <url-pattern>/receivevalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneratePhoto</servlet-name>
        <url-pattern>/generatephoto</url-pattern>
    </servlet-mapping>



</web-app>
