    </servlet>
    <servlet>
        <servlet-name>UserInputValidate</servlet-name>
        <servlet-class>terminals.controller.servlets.registrations.GiveValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReceiveValidate</servlet-name>
        <servlet-class>terminals.controller.servlets.registrations.ReceiveValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneratePhoto</servlet-name>
        <servlet-class>terminals.controller.servlets.registrations.GeneratePhoto</servlet-class>
    </servlet>





