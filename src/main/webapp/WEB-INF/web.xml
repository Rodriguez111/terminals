        <url-pattern>/selector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/adduser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateUser</servlet-name>
        <url-pattern>/updateuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUser</servlet-name>
        <url-pattern>/deleteuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainDeparts</servlet-name>
        <url-pattern>/departs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDepart</servlet-name>
        <url-pattern>/adddepart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDepart</servlet-name>
        <url-pattern>/deletedepart</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MainTerminals</servlet-name>
        <url-pattern>/terminals</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTerminal</servlet-name>
        <url-pattern>/addterminal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateTerminal</servlet-name>
        <url-pattern>/updateterminal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteTerminal</servlet-name>
        <url-pattern>/deleteterminal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Regs</servlet-name>
        <url-pattern>/regs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerateBarcode</servlet-name>
        <url-pattern>/generatebarcode</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayBarcodes</servlet-name>
